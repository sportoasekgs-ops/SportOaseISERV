{
  "name": "sportoase/iserv-sportoase",
  "type": "symfony-bundle",
  "description": "IServ module for booking school sports facilities with capacity management",
  "keywords": ["iserv", "symfony", "booking", "sports", "school"],
  "license": "MIT",
  "authors": [
    {
      "name": "SportOase Team",
      "email": "sportoase.kg@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "symfony/framework-bundle": "^6.4|^7.0",
    "symfony/config": "^6.4|^7.0",
    "symfony/dependency-injection": "^6.4|^7.0",
    "symfony/form": "^6.4|^7.0",
    "symfony/security-bundle": "^6.4|^7.0",
    "symfony/twig-bundle": "^6.4|^7.0",
    "symfony/validator": "^6.4|^7.0",
    "symfony/yaml": "^6.4|^7.0",
    "doctrine/doctrine-bundle": "^2.11",
    "doctrine/orm": "^2.17|^3.0",
    "doctrine/migrations": "^3.7",
    "symfony/mailer": "^6.4|^7.0",
    "symfony/mime": "^6.4|^7.0",
    "google/apiclient": "^2.18",
    "dompdf/dompdf": "^3.1",
    "knpuniversity/oauth2-client-bundle": "^2.18",
    "league/oauth2-client": "^2.7"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^6.4|^7.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "SportOase\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SportOase\\Tests\\": "tests/"
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": true
    }
  }
}
